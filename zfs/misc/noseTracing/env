#!/bin/bash

export ZFS_CONFIG_ROOT=`pwd`

export PYTHONPATH=$PYTHONPATH:`pwd`:`pwd`:`pwd`/../..:`pwd`/../../tests/nose-tests:`pwd`/../../syplog-py:/globalhome/loki/skola/python/nose
