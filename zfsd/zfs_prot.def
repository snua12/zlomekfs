		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (17, READLINK, readlink, zfs_fh,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (18, SYMLINK, symlink, symlink_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (19, MKNOD, mknod, mknod_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
# ifndef __KERNEL__
DEFINE_ZFS_PROC (20, AUTH_STAGE1, auth_stage1, auth_stage1_args,
		 AUTHENTICATION_NONE, DIR_REQUEST)
DEFINE_ZFS_PROC (21, AUTH_STAGE2, auth_stage2, auth_stage2_args,
		 AUTHENTICATION_STAGE_1, DIR_REQUEST)
DEFINE_ZFS_PROC (22, MD5SUM, md5sum, md5sum_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (23, PING, ping, data_buffer,
		 AUTHENTICATION_STAGE_1, DIR_REQUEST)
DEFINE_ZFS_PROC (24, FILE_INFO, file_info, zfs_fh,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (25, REINTEGRATE_ADD, reintegrate_add, reintegrate_add_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (26, REINTEGRATE_DEL, reintegrate_del, reintegrate_del_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (27, REINTEGRATE_SET, reintegrate_set, reintegrate_set_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
# endif
#endif
