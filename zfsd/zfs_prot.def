/* DEFINE_ZFS_PROC(NUMBER, NAME, FUNCTION, ARGS, AUTH, CALL_MODE)

   NUMBER = number of the function, sent in request
   NAME = name of the function, used for defining a symbolic alias for NUMBER
   FUNCTION = name of the function, used to declare the function
   ARGS = type of the argument (structure or void)
   AUTH = level of authentication needed to process the function
   CALL_MODE = either DIR_REQUEST or DIR_ONEWAY
*/

#if defined (ZFS_CALL_SERVER) || defined (ZFS_CALL_CLIENT)
# ifndef __KERNEL__
DEFINE_ZFS_PROC (0, NULL, null, void,
		 AUTHENTICATION_NONE, DIR_REQUEST)
# endif
DEFINE_ZFS_PROC (1, ROOT, root, void,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
# ifndef __KERNEL__
DEFINE_ZFS_PROC (2, VOLUME_ROOT, volume_root, volume_root_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
# endif
DEFINE_ZFS_PROC (3, GETATTR, getattr, zfs_fh,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (4, SETATTR, setattr, sattr_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (5, LOOKUP, lookup, dir_op_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (6, CREATE, create, create_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (7, OPEN, open, open_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (8, CLOSE, close, zfs_cap,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (9, READDIR, readdir, read_dir_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (10, MKDIR, mkdir, mkdir_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (11, RMDIR, rmdir, dir_op_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (12, RENAME, rename, rename_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (13, LINK, link, link_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (14, UNLINK, unlink, dir_op_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
DEFINE_ZFS_PROC (15, READ, read, read_args,
		 AUTHENTICATION_FINISHED, DIR_REQUEST)
